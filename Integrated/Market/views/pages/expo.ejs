<% include ../partials/pagestart %>
<% include ../partials/pageheader %>

<div class="expoPage">
    <div class="allExposView" ng-hide="showSingleExpo">
        <div class="secondDiv">
		<% if (expos.length > 0) { %>
            <% expos.forEach(function(expo){ %>
            <section  swinch-duration="1000" data-scroll
                     class="secondExpo repeated-item" id="expo<%= expo._id %>" swinch-offset="0" >
                <article>
                    <div class="expoData">
					 
                        <div>
                            <div class="singleExpoView" id="expo<%= expo._id %>">
                                <div class="container-fluid">
                                    <div class="row noGutter">

									<% expo.Sections.forEach(function(section){ %>
                                        <div class="col-xs-3 col-sm-3 col-custom5 repeated-item">
                                            <div class="image-wrapper">
                                                <a href="/Eg/Store/<%= section.Store.StoreName %>/<%= section.Store._id %>">
                                                    <img class="animated img-responsive animate scale wow zoomIn" data-wow-offset="30" data-wow-duration="1s" src="<%= section.Img %>" >
                                                </a>
                                            </div>
                                        </div>
									<% }); %>

                                    </div>
                                    <div class="expoDetails">
                                        <!-- Pagination -->
                                        <h6 class="text-center light expoPagingController<%= expo._id %>"
                                            onclick="toggle('<%= expo._id %>')">
                                            <img src="/images/rightarrow.png" />
                                        </h6>
                                        <h6 class="text-center light expoPagingControllerRemoval<%= expo._id %> hide opened"
                                            onclick="toggle('<%= expo._id %>')">
                                            <img src="/images/leftarrow.png" />
                                        </h6>
										<div class="whiteLayerContainer whiteLayerContainer<%= expo._id %>">
											<ul class="pagination-main clicked dvPageNumbers<%= expo._id %> " >
												<li  onclick="selectPage('<%= expo._id %>',1)" class="btnChangePage page1 active"><a>1</a></li>
												<li  onclick="selectPage('<%= expo._id %>',2)" class="btnChangePage page2"><a>2</a></li>
												<li  onclick="selectPage('<%= expo._id %>',3)" class="btnChangePage page3"><a>3</a></li>
												<li  onclick="selectPage('<%= expo._id %>',4)" class="btnChangePage page4"><a>4</a></li>
											</ul>
											<div class="whiteLayer"></div>
										</div>
                                    </div>
                                    <!-- //end Pagination -->
                                </div>
                            </div>
                        </div>
                    </div>
                </article>
            </section>



<div class="homepage-bar">
  <div class="container">
    <div class="row">
      <div class="col-md-4">
        <i class="icon-shipping"></i>
		<div class="text-area"><h3>FREE SHIPPING &amp; RETURN</h3><p>Free shipping on all orders over $99.</p></div>
      </div>
      <div class="col-md-4">
        <i class="icon-money-bag"></i>
		<div class="text-area"><h3>MONEY BACK GUARANTEE</h3><p>100% money back guarantee.</p></div>
      </div>
      <div class="col-md-4">
        <i class="icon-support"></i>
		<div class="text-area"><h3>ONLINE SUPPORT 24/7</h3><p>Lorem ipsum dolor sit amet.</p></div>
      </div>
    </div>
  </div>
</div>


            <% }); %>

			<div class="brands-slide" style="background-color:#f4f4f4;padding: 30px 0;margin-top:100px;"> 
  <div class="container" style="display:flex">
    <div style="margin:auto;">
		<img class="lazyOwl" width="166" height="63" alt="" src="http://www.newsmartwave.net/magento/porto/media/wysiwyg/porto/homepage/content/09/brand1.png" style="display: inline;">
		<img class="lazyOwl" width="166" height="63" alt="" src="http://www.newsmartwave.net/magento/porto/media/wysiwyg/porto/homepage/content/09/brand2.png" style="display: inline;">
		<img class="lazyOwl" width="166" height="63" alt="" src="http://www.newsmartwave.net/magento/porto/media/wysiwyg/porto/homepage/content/09/brand3.png" style="display: inline;">
		<img class="lazyOwl" width="166" height="63" alt="" src="http://www.newsmartwave.net/magento/porto/media/wysiwyg/porto/homepage/content/09/brand4.png" style="display: inline;">
		<img class="lazyOwl" width="166" height="63" alt="" src="http://www.newsmartwave.net/magento/porto/media/wysiwyg/porto/homepage/content/09/brand5.png" style="display: inline;">
		</div>

  </div>
</div>

			<% } else { %>
			 <p class="txtNoData"> No Data Available </p>
			 <% } %>
        </div>
    </div>



    
</div>


<% include ../partials/pagefooter %>
<% include ../partials/pageend %>
<script src="/scripts/expo.js"></script>
<% include ../partials/pagesocial %>

</body>

</html>