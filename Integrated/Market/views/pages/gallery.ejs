<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
    <!-- SLIDER REVOLUTION 4.x CSS SETTINGS -->
    <link rel="stylesheet" type="text/css" href="/rs-plugin/css/settings.css" media="screen" />
    <link rel="stylesheet" type="text/css" href="/rs-plugin/css/extralayers.css" media="screen" />
</head>
<body class="responsive">
    <% include ../partials/loaderandsidemenu %>

    <div id="outer">
        <div id="outer-canvas">
            <!-- Navbar -->
            <header>
                <% include ../partials/header %>
            </header>
            <!-- //end Navbar -->
            <!-- Social widgets -->
            <% include ../partials/socialwidgets %>
            <!-- //end Social widgets -->
            <!-- Breadcrumbs -->
            <section class="container">
                <nav class="breadcrumbs">
                    <span class="maincrumb"><a href="/eg/Home">Home</a></span>
                    <span class="maincrumb"><span class="divider">›</span><a href="/eg/store/almaksoud"><span> Al Maksoud</span></a></span>
                    <span class="maincrumb"><span class="divider">›</span><a href="/eg/g/home-furniture/1"><span> <%= gallery.Title %></span></a></span>
                </nav>
            </section>
            <!-- //end Breadcrumbs -->
            <!-- One column content -->
            <section class="container">

                <!-- Description -->
                <p><%= gallery.Description %></p>
                <!-- //end Description -->

                <!-- Products list -->
                <div class="row products-list">
                    <% products.forEach(function(product){ %>
                    <div class="product-preview">
                        <div class="preview animate scale">
                            <a href="/eg/p/<%= product.Name %>/<%= product._id %>" class="preview-image">
                                <img class="img-responsive animate scale" src="<%= product.Pictures[0].URL %>" width="270" height="328" alt=""></a>
                            <ul class="product-controls-list right">
                                <li><span class="label label-sale">SALE</span></li>
                                <li><span class="label">-20%</span></li>
                            </ul>
                            <ul class="product-controls-list right hide-right">
                                <li class="top-out"></li>
                                <li><a href="#" class="circle"><span class="icon-heart"></span></a></li>
                                <li><a href="#" class="circle"><span class="icon-justice"></span></a></li>
                                <li><a href="#" class="cart"><span class="icon-basket"></span></a></li>
                            </ul>
                            <a href="#" class="quick-view  hidden-xs"> <span class="rating"> <i class="icon-star-3"></i> <i class="icon-star-3"></i> <i class="icon-star-3"></i> <i class="icon-star-3"></i> <i class="icon-star-empty"></i> </span> <span class="icon-zoom-in-2"></span> Quick View </a>
                            <div class="quick-view visible-xs"> <span class="rating"> <i class="icon-star-3"></i> <i class="icon-star-3"></i> <i class="icon-star-3"></i> <i class="icon-star-3"></i> <i class="icon-star-empty"></i> </span></div>
                        </div>
                        <h3 class="title"><a href="/eg/p/<%= product.Name %>/<%= product._id %>"><%= product.Name %></a></h3>
                        <!--<span class="price old">$54.22</span> <span class="price new">$44.95</span>-->
                    </div>
                    <% }); %>
                   
                </div>
                <!-- //end Products list -->
                <!-- Product view compact -->
                <div class="product-view-ajax">
                    <div class="ajax-loader progress progress-striped active">
                        <div class="progress-bar progress-bar-danger" role="progressbar"></div>
                    </div>
                    <div class="layar"></div>
                    <div class="product-view-container"> </div>
                </div>
                <!-- //end Product view compact -->

            </section>
            <!-- //end One columns content -->
            <footer>
                <% include ../partials/footer %>
            </footer>
            <div id="outer-overlay"></div>
            <!-- //end Footer -->
        </div>
    </div>
    <% include ../partials/jsfiles %>

</html>
